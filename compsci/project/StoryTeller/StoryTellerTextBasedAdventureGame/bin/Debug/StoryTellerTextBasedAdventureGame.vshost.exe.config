<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <config>
    <gamePlay>
      <next_upper ConsoleKey='C' />
      <back ConsoleKey="Backspace" />
      <exit ConsoleKey="Escape" />
    </gamePlay>
    <text>
      <textType bool="true" /> <!-- Type out the text (true) or just prints it (false) -->
      <textFreq int="45000" /> <!-- The speed at which the text is output to the screen. [Default: 45000 (or 45ms between character type)] -->
    </text>
    <consoleColor>
      <foregroundColor ConsoleColor="Gray" />
      <backgroundColor ConsoleColor="Black" />
    </consoleColor>
  </config>
</configuration>
